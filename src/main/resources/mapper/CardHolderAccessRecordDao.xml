<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="io.renren.cms.dao.CardHolderAccessRecordDao">

	<sql id="base_column">
		cardHolderAccessRecord.`id`, cardHolderAccessRecord.`card_holder_id`, cardHolderAccessRecord.`openid`, cardHolderAccessRecord.`portrait`, cardHolderAccessRecord.`access_time`,
		cardHolderAccessRecord.`ctime`, cardHolderAccessRecord.`position`, cardHolderAccessRecord.`company`, cardHolderAccessRecord.`name`,
		cardHolderAccessRecord.`card_holder_openid`
	</sql>

	<!-- 可根据自己的需求，是否要使用 -->
	<resultMap type="io.renren.cms.entity.CardHolderAccessRecordEntity" id="cardHolderAccessRecordMap">
		<result property="id" column="id" />
		<result property="cardHolderId" column="card_holder_id" />
		<result property="openid" column="openid" />
		<result property="portrait" column="portrait" />
		<result property="accessTime" column="access_time" />
		<result property="ctime" column="ctime" />
		<result property="position" column="position" />
		<result property="company" column="company" />
		<result property="name" column="name" />
		<result property="cardHolderOpenid" column="card_holder_openid" />
	</resultMap>
	
	<select id="queryObjectByOpenidAndCardHolderId" resultType="io.renren.cms.entity.CardHolderAccessRecordEntity">
		SELECT * from tb_card_holder_access_record WHERE card_holder_id = #{cardHolderId} and openid = #{openid} LIMIT 1
	</select>

	<select id="queryObject" resultType="io.renren.cms.entity.CardHolderAccessRecordEntity">
		select * from tb_card_holder_access_record where id = #{value}
	</select>

	<select id="queryList" resultType="io.renren.cms.entity.CardHolderAccessRecordEntity">
		select * from tb_card_holder_access_record where 1 = 1

		<if test="cardHolderId != null and cardHolderId != ''"> and `card_holder_id` = #{cardHolderId}</if>
		<if test="openid != null and openid != ''"> and `openid` = #{openid}</if>
		<if test="position != null and position != ''"> and `position` = #{position}</if>
		<if test="company != null and company != ''"> and `company` = #{company}</if>
		<if test="name != null and name != ''"> and `name` = #{name}</if>
		<if test="cardHolderOpenid != null and cardHolderOpenid != ''"> and `card_holder_openid` = #{cardHolderOpenid}</if>

		<if test="sdate != null and sdate != '' and edate != null and edate != ''">
			and (access_time between CONCAT(#{sdate},' 00:00:00') and CONCAT(#{edate},' 23:59:59'))
		</if>

		<choose>
			<when test="sidx != null and sidx.trim() != ''">
				order by ${sidx} ${order}
			</when>
			<otherwise>
				order by access_time desc
			</otherwise>
		</choose>
		<if test="offset != null and limit != null">
			limit #{offset}, #{limit}
		</if>
	</select>

	<select id="queryTotal" resultType="int">
		select count(id) from tb_card_holder_access_record where 1 = 1
		<if test="cardHolderId != null and cardHolderId != ''"> and `card_holder_id` = #{cardHolderId}</if>
		<if test="openid != null and openid != ''"> and `openid` = #{openid}</if>
		<if test="position != null and position != ''"> and `position` = #{position}</if>
		<if test="company != null and company != ''"> and `company` = #{company}</if>
		<if test="name != null and name != ''"> and `name` = #{name}</if>
		<if test="cardHolderOpenid != null and cardHolderOpenid != ''"> and `card_holder_openid` = #{cardHolderOpenid}</if>

		<if test="sdate != null and sdate != '' and edate != null and edate != ''">
			and (access_time between CONCAT(#{sdate},' 00:00:00') and CONCAT(#{edate},' 23:59:59'))
		</if>
	</select>

	<insert id="save" parameterType="io.renren.cms.entity.CardHolderAccessRecordEntity" useGeneratedKeys="true" keyProperty="id">
		insert into tb_card_holder_access_record
		(
		`card_holder_id`,
		`openid`,
		`portrait`,
		`access_time`,
		`ctime`,
		`position`,
		`company`,
		`name`,
		`card_holder_openid`
		)
		values
		(
		#{cardHolderId},
		#{openid},
		#{portrait},
		#{accessTime},
		#{ctime},
		#{position},
		#{company},
		#{name},
		#{cardHolderOpenid}
		)
	</insert>

	<update id="update" parameterType="io.renren.cms.entity.CardHolderAccessRecordEntity">
		update tb_card_holder_access_record
		<set>
			<if test="cardHolderId != null">`card_holder_id` = #{cardHolderId}, </if>
			<if test="openid != null">`openid` = #{openid}, </if>
			<if test="portrait != null">`portrait` = #{portrait}, </if>
			<if test="accessTime != null">`access_time` = #{accessTime}, </if>
			<if test="ctime != null">`ctime` = #{ctime}, </if>
			<if test="position != null">`position` = #{position}, </if>
			<if test="company != null">`company` = #{company}, </if>
			<if test="name != null">`name` = #{name}, </if>
			<if test="cardHolderOpenid != null">`card_holder_openid` = #{cardHolderOpenid}</if>
		</set>
		where id = #{id}
	</update>

	<!-- 物理删除 -->
	<delete id="delete">
		delete from tb_card_holder_access_record where id = #{value}
	</delete>

	<!-- 批量物理删除 -->
	<delete id="deleteBatch">
		delete from tb_card_holder_access_record where id in
		<foreach item="id" collection="array" open="(" separator="," close=")">
			#{id}
		</foreach>
	</delete>

	<!-- 逻辑删除 -->
	<update id="logicDel">
		update tb_card_holder_access_record set is_del = 't' where id = #{value}
	</update>

	<!-- 批量逻辑删除 -->
	<update id="logicDelBatch">
		update tb_card_holder_access_record
		set is_del = 't'
		where id in
		<foreach item="id" collection="array" open="(" separator="," close=")">
			#{id}
		</foreach>
	</update>

</mapper>