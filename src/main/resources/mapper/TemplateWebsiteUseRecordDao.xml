<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="io.renren.cms.dao.TemplateWebsiteUseRecordDao">

	<sql id="base_column">
		templateWebsiteUseRecord.`id`, templateWebsiteUseRecord.`template_website_id`, templateWebsiteUseRecord.`member_id`, templateWebsiteUseRecord.`template_image_example`,
		templateWebsiteUseRecord.`avatar_url`, templateWebsiteUseRecord.`openid`, templateWebsiteUseRecord.`use_time`
	</sql>

	<!-- 可根据自己的需求，是否要使用 -->
	<resultMap type="io.renren.cms.entity.TemplateWebsiteUseRecordEntity" id="templateWebsiteUseRecordMap">
		<result property="id" column="id" />
		<result property="templateWebsiteId" column="template_website_id" />
		<result property="memberId" column="member_id" />
		<result property="templateImageExample" column="template_image_example" />
		<result property="avatarUrl" column="avatar_url" />
		<result property="openid" column="openid" />
		<result property="useTime" column="use_time" />
	</resultMap>
	
	<select id="queryListByTemplateWebsiteId" resultType="io.renren.cms.entity.TemplateWebsiteUseRecordEntity">
		SELECT
			*
		FROM
			tb_template_website_use_record
		WHERE
			template_website_id = #{value}
		AND avatar_url IS NOT NULL
		ORDER BY
			use_time DESC
		LIMIT 5
	</select>
	
	<select id="queryObjectByTemplateWebsiteIdAndMemberId" resultType="io.renren.cms.entity.TemplateWebsiteUseRecordEntity">
		select * from tb_template_website_use_record where template_website_id = #{templateWebsiteId} and member_id = #{memberId}
	</select>

	<select id="queryObject" resultType="io.renren.cms.entity.TemplateWebsiteUseRecordEntity">
		select * from tb_template_website_use_record where id = #{value}
	</select>

	<select id="queryList" resultType="io.renren.cms.entity.TemplateWebsiteUseRecordEntity">
		select * from tb_template_website_use_record where 1 = 1

		<if test="templateWebsiteId != null and templateWebsiteId != ''"> and `template_website_id` = #{templateWebsiteId}</if>
		<if test="memberId != null and memberId != ''"> and `member_id` = #{memberId}</if>
		<if test="openid != null and openid != ''"> and `openid` = #{openid}</if>

		<if test="sdate != null and sdate != '' and edate != null and edate != ''">
			and (use_time between CONCAT(#{sdate},' 00:00:00') and CONCAT(#{edate},' 23:59:59'))
		</if>

		<choose>
			<when test="sidx != null and sidx.trim() != ''">
				order by ${sidx} ${order}
			</when>
			<otherwise>
				order by use_time desc
			</otherwise>
		</choose>
		<if test="offset != null and limit != null">
			limit #{offset}, #{limit}
		</if>
	</select>

	<select id="queryTotal" resultType="int">
		select count(id) from tb_template_website_use_record where 1 = 1
		<if test="templateWebsiteId != null and templateWebsiteId != ''"> and `template_website_id` = #{templateWebsiteId}</if>
		<if test="memberId != null and memberId != ''"> and `member_id` = #{memberId}</if>
		<if test="openid != null and openid != ''"> and `openid` = #{openid}</if>

		<if test="sdate != null and sdate != '' and edate != null and edate != ''">
			and (use_time between CONCAT(#{sdate},' 00:00:00') and CONCAT(#{edate},' 23:59:59'))
		</if>
	</select>

	<insert id="save" parameterType="io.renren.cms.entity.TemplateWebsiteUseRecordEntity">
		insert into tb_template_website_use_record
		(
		`id`,
		`template_website_id`,
		`member_id`,
		`template_image_example`,
		`avatar_url`,
		`openid`,
		`use_time`
		)
		values
		(
		#{id},
		#{templateWebsiteId},
		#{memberId},
		#{templateImageExample},
		#{avatarUrl},
		#{openid},
		#{useTime}
		)
	</insert>

	<update id="update" parameterType="io.renren.cms.entity.TemplateWebsiteUseRecordEntity">
		update tb_template_website_use_record
		<set>
			<if test="templateWebsiteId != null">`template_website_id` = #{templateWebsiteId}, </if>
			<if test="memberId != null">`member_id` = #{memberId}, </if>
			<if test="templateImageExample != null">`template_image_example` = #{templateImageExample}, </if>
			<if test="avatarUrl != null">`avatar_url` = #{avatarUrl}, </if>
			<if test="openid != null">`openid` = #{openid}, </if>
			<if test="useTime != null">`use_time` = #{useTime}</if>
		</set>
		where id = #{id}
	</update>

	<!-- 物理删除 -->
	<delete id="delete">
		delete from tb_template_website_use_record where id = #{value}
	</delete>

	<!-- 批量物理删除 -->
	<delete id="deleteBatch">
		delete from tb_template_website_use_record where id in
		<foreach item="id" collection="array" open="(" separator="," close=")">
			#{id}
		</foreach>
	</delete>

	<!-- 逻辑删除 -->
	<update id="logicDel">
		update tb_template_website_use_record set is_del = 't' where id = #{value}
	</update>

	<!-- 批量逻辑删除 -->
	<update id="logicDelBatch">
		update tb_template_website_use_record
		set is_del = 't'
		where id in
		<foreach item="id" collection="array" open="(" separator="," close=")">
			#{id}
		</foreach>
	</update>

	<select id="queryListVo" resultType="io.renren.api.vo.TemplateWebsiteUseRecordEntityVo">
		select ttwur.*,ttwl.qrcode as qrcode from tb_template_website_use_record ttwur
		left join tb_template_website_layout ttwl
		on ttwur.openid = ttwl.openid
		where 1 = 1

		<if test="templateWebsiteId != null and templateWebsiteId != ''"> and ttwur.`template_website_id` = #{templateWebsiteId}</if>
		<if test="memberId != null and memberId != ''"> and ttwur.`member_id` = #{memberId}</if>
		<if test="openid != null and openid != ''"> and ttwur.`openid` = #{openid}</if>

		<if test="sdate != null and sdate != '' and edate != null and edate != ''">
			and (ttwur.use_time between CONCAT(#{sdate},' 00:00:00') and CONCAT(#{edate},' 23:59:59'))
		</if>

		<choose>
			<when test="sidx != null and sidx.trim() != ''">
				order by ${sidx} ${order}
			</when>
			<otherwise>
				order by ttwur.use_time desc
			</otherwise>
		</choose>
		<if test="offset != null and limit != null">
			limit #{offset}, #{limit}
		</if>
	</select>

</mapper>