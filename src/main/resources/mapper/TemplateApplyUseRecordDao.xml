<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="io.renren.cms.dao.TemplateApplyUseRecordDao">

	<sql id="base_column">
		templateApplyUseRecord.`id`, templateApplyUseRecord.`template_apply_id`, templateApplyUseRecord.`member_id`, templateApplyUseRecord.`template_image_example`, templateApplyUseRecord.`avatar_url`,
		templateApplyUseRecord.`openid`, templateApplyUseRecord.`use_time`
	</sql>

	<!-- 可根据自己的需求，是否要使用 -->
	<resultMap type="io.renren.cms.entity.TemplateApplyUseRecordEntity" id="templateApplyUseRecordMap">
		<result property="id" column="id" />
		<result property="templateApplyId" column="template_apply_id" />
		<result property="memberId" column="member_id" />
		<result property="templateImageExample" column="template_image_example" />
		<result property="avatarUrl" column="avatar_url" />
		<result property="openid" column="openid" />
		<result property="useTime" column="use_time" />
	</resultMap>

	<select id="queryListByTemplateApplyId" resultType="io.renren.cms.entity.TemplateApplyUseRecordEntity">
		SELECT
			*
		FROM
			tb_template_apply_use_record
		WHERE
			template_apply_id = #{value}
		AND avatar_url IS NOT NULL
		GROUP BY
			openid
		ORDER BY
			use_time DESC
		LIMIT 5
	</select>

	<select id="queryObject" resultType="io.renren.cms.entity.TemplateApplyUseRecordEntity">
		select * from tb_template_apply_use_record where id = #{value}
	</select>

	<select id="queryList" resultType="io.renren.cms.entity.TemplateApplyUseRecordEntity">
		select * from tb_template_apply_use_record where 1 = 1

		<if test="templateApplyId != null and templateApplyId != ''"> and `template_apply_id` = #{templateApplyId}</if>
		<if test="memberId != null and memberId != ''"> and `member_id` = #{memberId}</if>
		<if test="templateImageExample != null and templateImageExample != ''"> and `template_image_example` = #{templateImageExample}</if>
		<if test="avatarUrl != null and avatarUrl != ''"> and `avatar_url` = #{avatarUrl}</if>
		<if test="openid != null and openid != ''"> and `openid` = #{openid}</if>
		<if test="useTime != null and useTime != ''"> and `use_time` = #{useTime}</if>

		<if test="sdate != null and sdate != '' and edate != null and edate != ''">
			and (ctime between CONCAT(#{sdate},' 00:00:00') and CONCAT(#{edate},' 23:59:59'))
		</if>

		<choose>
			<when test="sidx != null and sidx.trim() != ''">
				order by ${sidx} ${order}
			</when>
			<otherwise>
				order by id desc
			</otherwise>
		</choose>
		<if test="offset != null and limit != null">
			limit #{offset}, #{limit}
		</if>
	</select>

	<select id="queryTotal" resultType="int">
		select count(id) from tb_template_apply_use_record where 1 = 1
		<if test="templateApplyId != null and templateApplyId != ''"> and `template_apply_id` = #{templateApplyId}</if>
		<if test="memberId != null and memberId != ''"> and `member_id` = #{memberId}</if>
		<if test="templateImageExample != null and templateImageExample != ''"> and `template_image_example` = #{templateImageExample}</if>
		<if test="avatarUrl != null and avatarUrl != ''"> and `avatar_url` = #{avatarUrl}</if>
		<if test="openid != null and openid != ''"> and `openid` = #{openid}</if>
		<if test="useTime != null and useTime != ''"> and `use_time` = #{useTime}</if>

		<if test="sdate != null and sdate != '' and edate != null and edate != ''">
			and (ctime between CONCAT(#{sdate},' 00:00:00') and CONCAT(#{edate},' 23:59:59'))
		</if>
	</select>

	<insert id="save" parameterType="io.renren.cms.entity.TemplateApplyUseRecordEntity" useGeneratedKeys="true" keyProperty="id">
		insert into tb_template_apply_use_record
		(
		`template_apply_id`,
		`member_id`,
		`template_image_example`,
		`avatar_url`,
		`openid`,
		`use_time`
		)
		values
		(
		#{templateApplyId},
		#{memberId},
		#{templateImageExample},
		#{avatarUrl},
		#{openid},
		#{useTime}
		)
	</insert>

	<update id="update" parameterType="io.renren.cms.entity.TemplateApplyUseRecordEntity">
		update tb_template_apply_use_record
		<set>
			<if test="templateApplyId != null">`template_apply_id` = #{templateApplyId}, </if>
			<if test="memberId != null">`member_id` = #{memberId}, </if>
			<if test="templateImageExample != null">`template_image_example` = #{templateImageExample}, </if>
			<if test="avatarUrl != null">`avatar_url` = #{avatarUrl}, </if>
			<if test="openid != null">`openid` = #{openid}, </if>
			<if test="useTime != null">`use_time` = #{useTime}</if>
		</set>
		where id = #{id}
	</update>

	<!-- 物理删除 -->
	<delete id="delete">
		delete from tb_template_apply_use_record where id = #{value}
	</delete>

	<!-- 批量物理删除 -->
	<delete id="deleteBatch">
		delete from tb_template_apply_use_record where id in
		<foreach item="id" collection="array" open="(" separator="," close=")">
			#{id}
		</foreach>
	</delete>

	<!-- 逻辑删除 -->
	<update id="logicDel">
		update tb_template_apply_use_record set is_del = 't' where id = #{value}
	</update>

	<!-- 批量逻辑删除 -->
	<update id="logicDelBatch">
		update tb_template_apply_use_record
		set is_del = 't'
		where id in
		<foreach item="id" collection="array" open="(" separator="," close=")">
			#{id}
		</foreach>
	</update>

</mapper>